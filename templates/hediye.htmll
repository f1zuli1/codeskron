    .socks {
        position: absolute;
        top: 80px;
        left: 10px;
        z-index: 1;
        transform: rotate(10deg);
    }
    .socks:after {
        content: '';
        position: absolute;
        width: 15px;
        height: 35px;
        background: #E54B4B;
        border-radius: 5px;
    }
    .socks:before {
        content: '';
        position: absolute;
        top: 20px;
        left: 0;
        width: 25px;
        height: 15px;
        border-radius: 30px;
        background: #E54B4B;
    }
    .tree-base {
        width: 450px;
        height: 45px;
        margin: auto;
        background: rgba(0, 0, 0, 0.15);
        position: relative;
        top: 72px;
        border-radius: 100%;
    }
    .tree-base:after {
        content: '';
        width: 50px;
        height: 100px;
        background: #84714F;
        position: absolute;
        top: -77px;
        left: 0;
        margin: auto;
        right: 0;
    }
    
    .gift {
        width: 100px;
        height: 69px;
        transform: translate3d(-50%, -50%, 0);
    }
    .gift-top {
        position: relative;
        width: 115%;
        height: 25%;
        left: -2px;
        background: #ffc113;
        border-radius: 5px 5px 0 0;
        transform: translate3d(-5%, 0, 0);
        background-image: linear-gradient(to right, #e4ad13 0%, #e4ad13 44%, #FFC113 44%, #FFC113 56%, #e4ad13 56%, #e4ad13 100%)
    }
    .gift-top::before, .gift-top::after {
        content: "";
        width: 15px;
        height: 15px;
        border: 10px solid #FFC113;
        border-radius: 50% 50% 0 50%;
        position: absolute;
        top: 0;
        left: 50%;
        transform: translate3d(-100%, -100%, 0) skew(10deg, 10deg);
    }
    .gift-top::after {
        border-radius: 50% 50% 50% 0;
        transform: translate3d(0, -100%, 0) skew(-10deg, -10deg);
    }
    .gift-box {
        width: 100%;
        height: 75%;
        background-image: linear-gradient(to right, #ffc113 0%, #ffc113 45%, #e4ad13 45%, #e4ad13 55%, #ffc113 55%, #ffc113 100%);
        border-radius: 0 0 5px 5px;
    }

.cta {
    border: none;
    background: none;
    cursor: pointer;
    position: relative;
    top: -550px;
    left: 400px;
    font-size: 20px;
    z-index: 10;
}

.cta span {
  padding-bottom: 7px;
  letter-spacing: 4px;
  font-size: 14px;
  padding-right: 15px;
  text-transform: uppercase;
}

.cta svg {
  transform: translateX(-8px);
  transition: all 0.3s ease;
}

.cta:hover svg {
  transform: translateX(0);
}

.cta:active svg {
  transform: scale(0.9);
}

.hover-underline-animation {
  position: relative;
  color: white;
  padding-bottom: 20px;
}

.hover-underline-animation:after {
  content: "";
  position: absolute;
  width: 100%;
  transform: scaleX(0);
  height: 2px;
  bottom: 0;
  left: 0;
  background-color: whitesmoke;
  transform-origin: bottom right;
  transition: transform 0.25s ease-out;
}

.cta:hover .hover-underline-animation:after {
  transform: scaleX(1);
  transform-origin: bottom left;
}

.hover-underline-animation1 {
